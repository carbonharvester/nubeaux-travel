# Copy this file to .env and fill in your actual values
# These are required for the Instagram sync functions to work

# Apify API Token (from apify.com)
APIFY_API_TOKEN=your_apify_token_here

# Supabase (from your Supabase project settings)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_key_here

# Admin auth for privileged endpoints
ADMIN_API_KEY=choose_a_long_random_admin_key

# Retell configuration
RETELL_API_KEY=your_retell_api_key
RETELL_AGENT_ID=your_retell_agent_id
RETELL_WEBHOOK_SECRET=choose_a_long_random_retell_webhook_secret

# Apify webhook auth
SYNC_WEBHOOK_SECRET=choose_a_long_random_sync_webhook_secret

# Cloudinary (for media uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
